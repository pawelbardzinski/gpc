<% @page_title = "Edit Account Page" %>

<div class="Login">
	<h4>edit account</h4>
	<%= form_tag(:action => 'edit_account') do %>
	<table>
		<tr>
			<td><%= label_tag(:new_username) %></td>
			<td><%= text_field_tag(:login)%></td>
		</tr>
		<tr>
			<td><%= label_tag(:new_password) %></td>
			<td><%= password_field_tag(:password) %></td>
		</tr>
	</table>
	<td><%= submit_tag("submit changes")%></td>
	<% end %>
</div>
